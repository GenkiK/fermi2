 cov = np.sum((xs - x_ave) * (ys - y_ave))\n","    var = np.sum((xs - x_ave)**2)\n","    slope = cov / var\n","    intercept = y_ave - x_ave * slope\n","    return intercept, slope"],"metadata":{"id":"Y-ftIXaSNUBc"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["_, degeneracies = calc_degeneracy(states12_25)"],"metadata":{"id":"cgtxgurW6YcN"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["plt.figure(figsize=(10, 6))\n","plt.title(r\"$\\ln{\\rho_E(E)}$\")\n","plt.semilogy(scores, degeneracies, '.-')\n","plt.xlabel(\"E [eV]\")\n","plt.show()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":427},"id":"yI94yjSj9IgI","executionInfo":{"status":"ok","timestamp":1644454497615,"user_tz":-540,"elapsed":816,"user":{"displayName":"木下元希","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"04872797509385535481"}},"outputId":"b797e80f-9537-4a20-f430-a6fc7458895c"},"execution_count":null,"outputs":[{"output_type":"display_data","data":{"image/png":"\n","text/plain":["<Figure size 720x432 with 1 Axes>"]},"metadata":{}}]},{"cell_type":"code","source":["head = 24\n","tail = None # 一番後ろのデータまで見る\n","# tail = -1\n","# a, b = solve_normal_equation(xs=bins[head:-2], ys=np.log(density_hist[head:-1])) # y = a + bx\n","# a, b = solve_normal_equation(xs=bins_centerized[head:tail], ys=np.log(density_hist[head:tail])) # y = a + bx\n","a, b = solve_normal_equation(xs=scores[head:tail], ys=np.log(degeneracies[head:tail])) # y = a + bx\n","rho_0 = np.exp(a)\n","eps_0 = 1/b\n","print(f\"rho_0 = {rho_0}\\neps_0 = {eps_0}\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"YxncTTibNTva","executionInfo":{"status":"ok","timestamp":1644454543516,"user_tz":-540,"elapsed":268,"user":{"displayName":"木下元希","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"04872797509385535481"}},"outputId":"3b57023d-d5e4-4c32-ba95-29c657182b5b"},"execution_count":null,"outputs":[{"output_type":"st